   <ImageView
        android:id="@+id/delprovyta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:padding="10dp"
        android:layout_below="@+id/arrow" />

    <ImageView
        android:id="@+id/east"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/delprovyta" />

    <ImageView
        android:id="@+id/north"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/delprovyta"
        android:layout_toRightOf="@+id/east" />

    <ImageView
        android:id="@+id/west"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:layout_below="@+id/east" />

    <ImageView
        android:id="@+id/south"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
         android:layout_toRightOf="@+id/west"
        android:layout_below="@+id/north" />
    -->
        <TextView
        android:id="@+id/oldpichead"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/arrow"
        android:layout_centerHorizontal="true"
        android:text="Arkivbilder (öst,väst,nord,syd)"
        android:textAppearance="?android:attr/textAppearanceMedium" />
    

    <GridView
        android:id="@+id/gridview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/oldpichead"
        android:columnWidth="200dp"
        android:horizontalSpacing="10dp"
        android:numColumns="auto_fit"
        android:stretchMode="columnWidth"
        android:layout_centerHorizontal="true"
        android:verticalSpacing="10dp" />

</RelativeLayout>